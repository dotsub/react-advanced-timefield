{
  "name": "react-advanced-timefield",
  "version": "3.6.7",
  "description": "Advanced react time input field",
  "author": "Adolfo Vazquez <adolfo.software@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && cd ./demo && npm run build && cd -",
    "test": "jest",
    "lint": "eslint --ignore-path .gitignore --ignore-pattern docs -- .",
    "prettier": "prettier --ignore-path .gitignore --write './**/*.js'",
    "typecheck": "tsc",
    "format": "npm run prettier && npm run lint",
    "cover": "jest --coverage",
    "release:patch": "npm run format && npm test && npm run build && npx standard-version -r patch && git push --follow-tags origin master",
    "release:minor": "npm run format && npm test && npm run build && npx standard-version -r minor && git push --follow-tags origin master",
    "release:major": "npm run format && npm test && npm run build && npx standard-version -r major && git push --follow-tags origin master",
    "update-contributors": "npx contributor"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dotsub/react-advanced-timefield.git"
  },
  "keywords": [
    "react",
    "time",
    "timefield",
    "timeinput",
    "input"
  ],
  "bugs": {
    "url": "https://github.com/dotsub/react-advanced-timefield/issues"
  },
  "homepage": "https://github.com/dotsub/react-advanced-timefield#readme",
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/enzyme": "3.10.11",
    "@types/jest": "27.4.0",
    "@types/react": "17.0.2",
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/parser": "5.11.0",
    "@babel/eslint-parser": "7.17.0",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-flow": "7.16.7",
    "@babel/preset-react": "7.16.7",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "babel-cli": "6.26.0",
    "babel-jest": "26.1.0",
    "enzyme": "3.11.0",
    "eslint": "8.9.0",
    "eslint-config-fbjs": "3.1.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-relay": "1.8.3",
    "jest": "27.5.1",
    "jsdom": "19.0.0",
    "jsdom-global": "3.0.2",
    "prettier": "2.5.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-test-renderer": "17.0.2",
    "ts-jest": "27.1.3",
    "typescript": "4.4.4"
  },
  "dependencies": {
    "@types/enzyme-adapter-react-16": "1.0.6"
  },
  "contributors": [
    "Anton Fisher (https://github.com/antonfisher)",
    "Nicholas Yang (https://github.com/NicholasLYang)",
    "Adolfo Vazquez (https://github.com/adolfov)"
  ],
  "directories": {
    "doc": "docs",
    "test": "tests"
  }
}
